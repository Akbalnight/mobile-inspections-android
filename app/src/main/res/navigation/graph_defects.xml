<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/graph_defects"
    app:startDestination="@id/defectListFragment">

    <include app:graph="@navigation/graph_defects_detail" />

    <fragment
        android:id="@+id/defectListFragment"
        android:name="ru.madbrains.inspection.ui.main.defects.defectlist.DefectListFragment"
        android:label="DefectListFragment"
        tools:layout="@layout/fragment_defect_list">
        <action
            android:id="@+id/action_defectListFragment_to_detailFragment"
            app:destination="@id/graph_defects_detail"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>


</navigation>